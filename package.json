{
	"name": "gguf",
	"displayName": "gguf",
	"description": "gguf code bot extension",
	"version": "0.2.9",
	"publisher": "gguf",
	"author": {
		"name": "gguf"
	},
	"license": "MIT",
	"icon": "assets/icons/icon.png",
	"engines": {
		"vscode": "^1.84.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gguf-org/monster"
	},
	"homepage": "https://gguf.org",
	"categories": [
		"Machine Learning"
	],
	"keywords": [
		"openclaw"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished",
		"workspaceContains:evals.env"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"icons": {
			"coder-icon": {
				"description": "coder",
				"default": {
					"fontPath": "assets/icons/coder-bot.woff",
					"fontCharacter": "\\e901"
				}
			}
		},
		"walkthroughs": [
			{
				"id": "CoderWalkthrough",
				"title": "Meet Coder, your new coding partner",
				"description": "Coder codes like a developer because it thinks like one. Here are 5 ways to put it to work:",
				"steps": [
					{
						"id": "welcome",
						"title": "Start with a Goal, Not Just a Prompt",
						"description": "Tell Coder what you want to achieve. It plans, asks, and then codes, like a true partner.",
						"media": {
							"markdown": "walkthrough/step1.md"
						}
					},
					{
						"id": "learn",
						"title": "Let Coder Learn Your Codebase",
						"description": "Point Coder to your project. It builds understanding to make smart, context-aware changes.",
						"media": {
							"markdown": "walkthrough/step2.md"
						}
					},
					{
						"id": "advanced-features",
						"title": "Always Use the Best AI Models",
						"description": "Coder empowers you with State-of-the-Art AI, connecting to top models (Anthropic, Gemini, OpenAI & more).",
						"media": {
							"markdown": "walkthrough/step3.md"
						}
					},
					{
						"id": "mcp",
						"title": "Extend with Powerful Tools (MCP)",
						"description": "Connect to databases, APIs, or discover new capabilities in the MCP Marketplace.",
						"media": {
							"markdown": "walkthrough/step4.md"
						}
					},
					{
						"id": "getting-started",
						"title": "You're Always in Control",
						"description": "Review Coder's plans and diffs. Approve changes before they happen. No surprises.",
						"media": {
							"markdown": "walkthrough/step5.md"
						},
						"content": {
							"path": "walkthrough/step5.md"
						}
					}
				]
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gguf-dev-ActivityBar",
					"title": "Coder",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"views": {
			"gguf-dev-ActivityBar": [
				{
					"type": "webview",
					"id": "gguf-dev.SidebarProvider",
					"name": "",
					"icon": "assets/icons/icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "gguf-editor.open",
				"title": "Open gguf-editor",
				"category": "gguf-editor"
			},
			{
				"command": "coder.connect",
				"title": "Connect to Coder",
				"category": "gguf-coder"
			},
			{
				"command": "coder.disconnect",
				"title": "Disconnect from Coder",
				"category": "gguf-coder"
			},
			{
				"command": "coder.startCli",
				"title": "Start Coder CLI",
				"category": "gguf-coder"
			},
			{
				"command": "coder.askAboutCode",
				"title": "Ask Coder about this",
				"category": "gguf-coder"
			},
			{
				"command": "coder.explainCode",
				"title": "Explain this code",
				"category": "gguf-coder"
			},
			{
				"command": "coder.refactorCode",
				"title": "Refactor this code",
				"category": "gguf-coder"
			},
			{
				"command": "coder.openSplitTerminal",
				"title": "Open Coder Terminal",
				"category": "gguf-coder",
				"icon": "assets/images/logo.png"
			},
			{
				"command": "coder.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "coder.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "coder.dev.createTestTasks",
				"title": "Create Test Tasks",
				"category": "Coder",
				"when": "coder.isDevMode"
			},
			{
				"command": "coder.dev.expireMcpOAuthTokens",
				"title": "Expire MCP OAuth Tokens (for testing)",
				"category": "Coder",
				"when": "coder.isDevMode"
			},
			{
				"command": "coder.addToChat",
				"title": "Add to Coder",
				"category": "Coder"
			},
			{
				"command": "coder.addTerminalOutputToChat",
				"title": "Add to Coder",
				"category": "Coder"
			},
			{
				"command": "coder.focusChatInput",
				"title": "Jump to Chat Input",
				"category": "Coder"
			},
			{
				"command": "coder.generateGitCommitMessage",
				"title": "Generate Commit Message with Coder",
				"category": "Coder",
				"icon": "$(coder-icon)"
			},
			{
				"command": "coder.abortGitCommitMessage",
				"title": "Generate Commit Message with Coder - Stop",
				"category": "Coder",
				"icon": "$(debug-stop)"
			},
			{
				"command": "coder.ide.explainCode",
				"title": "Explain with Coder",
				"category": "Coder"
			},
			{
				"command": "coder.improveCode",
				"title": "Improve with Coder",
				"category": "Coder"
			},
			{
				"command": "coder.openWalkthrough",
				"title": "Open Walkthrough",
				"category": "Coder"
			},
			{
				"command": "coder.reconstructTaskHistory",
				"title": "Reconstruct Task History",
				"category": "Coder"
			},
			{
				"command": "coder.reviewComment.reply",
				"title": "Reply",
				"category": "Coder",
				"enablement": "!commentIsEmpty"
			},
			{
				"command": "coder.reviewComment.addToChat",
				"title": "Add to Coder Chat",
				"category": "Coder",
				"icon": "$(link-external)"
			},
			{
				"command": "openclawbot.showMenu",
				"title": "Bot: Show Menu"
			}
		],
		"keybindings": [
			{
				"command": "editor.action.submitComment",
				"key": "enter",
				"when": "commentEditorFocused && commentController == coder-ai-review && !commentIsEmpty"
			},
			{
				"command": "coder.addToChat",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "editorHasSelection"
			},
			{
				"command": "coder.focusChatInput",
				"key": "cmd+'",
				"mac": "cmd+'",
				"win": "ctrl+'",
				"linux": "ctrl+'",
				"when": "!editorHasSelection"
			}
		],
		"menus": {
			"explorer/context": [
				{
				"command": "gguf-editor.open",
				"group": "gguf-editor",
				"when": "resourceExtname == .gguf"
				}
			],
			"view/title": [
				{
					"command": "coder.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == gguf-dev.SidebarProvider"
				},
				{
					"command": "coder.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == gguf-dev.SidebarProvider"
				}
			],
			"editor/context": [
				{
					"command": "coder.addToChat",
					"group": "navigation",
					"when": "editorHasSelection"
				},
				{
					"submenu": "coder.submenu",
					"group": "gguf-coder"
				}
			],
			"editor/title": [
				{
					"command": "coder.openSplitTerminal",
					"group": "navigation"
				}
			],
			"terminal/context": [
				{
					"command": "coder.addTerminalOutputToChat",
					"group": "navigation"
				}
			],
			"scm/title": [
				{
					"command": "coder.generateGitCommitMessage",
					"group": "navigation",
					"when": "config.git.enabled && scmProvider == git && !coder.isGeneratingCommit"
				},
				{
					"command": "coder.abortGitCommitMessage",
					"group": "navigation",
					"when": "config.git.enabled && scmProvider == git && coder.isGeneratingCommit"
				}
			],
			"coder.submenu": [
				{
					"command": "coder.askAboutCode",
					"when": "editorHasSelection"
				},
				{
					"command": "coder.explainCode",
					"when": "editorHasSelection"
				},
				{
					"command": "coder.refactorCode",
					"when": "editorHasSelection"
				}
			]
		},
		"submenus": [
			{
				"id": "coder.submenu",
				"label": "Coder"
			}
		],
		"commandPalette": [
			{
				"command": "coder.generateGitCommitMessage",
				"when": "config.git.enabled && !coder.isGeneratingCommit"
			},
			{
				"command": "coder.abortGitCommitMessage",
				"when": "config.git.enabled && coder.isGeneratingCommit"
			},
			{
				"command": "coder.reviewComment.reply",
				"when": "false"
			}
		],
		"comments/commentThread/context": [
			{
				"command": "coder.reviewComment.reply",
				"group": "inline",
				"when": "commentController == coder-ai-review"
			}
		],
		"comments/commentThread/title": [
			{
				"command": "coder.reviewComment.addToChat",
				"group": "inline",
				"when": "commentController == coder-ai-review"
			}
		],
		"configuration": {
			"title": "Coder",
			"properties": {
				"coder.serverPort": {
					"type": "number",
					"default": 51820,
					"description": "WebSocket server port for communication with Coder CLI"
				},
				"coder.autoConnect": {
					"type": "boolean",
					"default": false,
					"description": "Automatically connect to Coder CLI on startup"
				},
				"coder.autoStartCli": {
					"type": "boolean",
					"default": false,
					"description": "Automatically start Coder CLI if not running"
				},
				"coder.showDiffPreview": {
					"type": "boolean",
					"default": true,
					"description": "Show diff preview before applying file changes"
				},
				"gguf-editor.maxArrayElements": {
				"type": "number",
				"default": 25,
				"scope": "window",
				"description": "Maximum number of elements to display in an array."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"build": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.mjs",
		"compile-standalone": "npm run check-types && npm run lint && node esbuild.mjs --standalone",
		"compile-standalone-npm": "npm run protos && npm run protos-go && npm run check-types && npm run lint && node esbuild.mjs --standalone",
		"compile-cli": "scripts/build-cli.sh",
		"compile-cli-all-platforms": "scripts/build-cli-all-platforms.sh",
		"compile-cli-man-page": "pandoc cli/man/coder.1.md -s -t man -o cli/man/coder.1",
		"test:install": "bash scripts/test-install.sh",
		"dev:cli:watch": "node scripts/dev-cli-watch.mjs",
		"postcompile-standalone": "node scripts/package-standalone.mjs",
		"postcompile-standalone-npm": "node scripts/package-npm.mjs",
		"dev": "npm run protos && npm run watch",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.mjs --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run build:webview && npm run lint && node esbuild.mjs --production",
		"protos": "node scripts/build-proto.mjs",
		"protos-go": "node scripts/build-go-proto.mjs",
		"protos-python": "node scripts/build-python-proto.mjs",
		"cli-providers": "node scripts/cli-providers.mjs",
		"download-ripgrep": "node scripts/download-ripgrep.mjs",
		"postprotos": "biome format src/shared/proto src/core/controller src/hosts/ webview-ui/src/services src/generated --write --no-errors-on-unmatched",
		"clean:build": "rimraf dist dist-standalone webview-ui/build src/generated out/",
		"clean:deps": "rimraf node_modules webview-ui/node_modules",
		"clean:all": "npm run clean:build && npm run clean:deps",
		"compile-tests": "node ./scripts/build-tests.js",
		"watch-tests": "tsc -p . -w --outDir out",
		"check-types": "npm run protos && npx tsc --noEmit && cd webview-ui && npx tsc --noEmit",
		"lint": "biome lint --no-errors-on-unmatched --files-ignore-unknown=true --diagnostic-level=error",
		"lint:proto": "bash ./scripts/proto-lint.sh",
		"format": "biome format --changed --no-errors-on-unmatched --files-ignore-unknown=true --diagnostic-level=error",
		"format:fix": "biome check --changed --no-errors-on-unmatched --files-ignore-unknown=true --write",
		"fix:all": "biome check --no-errors-on-unmatched --files-ignore-unknown=true --write --diagnostic-level=error --unsafe",
		"ci:check-all": "npm-run-all -p check-types lint format",
		"ci:build": "npm run protos && npm run build:webview && node esbuild.mjs && npm run compile-tests",
		"pretest": "npm run compile && npm run compile-tests && npm run compile-standalone && npm run lint",
		"test": "npm-run-all test:unit test:integration",
		"test:integration": "vscode-test",
		"test:unit": "cross-env TS_NODE_PROJECT=./tsconfig.unit-test.json mocha",
		"test:coverage": "vscode-test --coverage",
		"test:sca-server": "npx tsx watch scripts/test-standalone-core-api-server.ts",
		"test:tp-orchestrator": "npx tsx scripts/testing-platform-orchestrator.ts",
		"e2e": "playwright test -c playwright.config.ts",
		"test:e2e:build": "vsce package --allow-package-secrets sendgrid --out dist/e2e.vsix",
		"test:e2e": "playwright install && npm run test:e2e:build && node src/test/e2e/utils/build.mjs && playwright test",
		"test:e2e:optimal": "npm run test:e2e:build && node src/test/e2e/utils/build.mjs && playwright test",
		"test:e2e:ui": "npx tsx scripts/interactive-playwright.ts",
		"install:all": "npm install && cd webview-ui && npm install",
		"dev:webview": "cd webview-ui && npm run dev",
		"build:webview": "cd webview-ui && npm run build",
		"test:webview": "cd webview-ui && npm run test",
		"publish:marketplace": "vsce publish --allow-package-secrets sendgrid && ovsx publish",
		"publish:marketplace:prerelease": "vsce publish --allow-package-secrets sendgrid --pre-release && ovsx publish --pre-release",
		"publish:marketplace:nightly": "node ./scripts/publish-nightly.mjs",
		"prepare": "husky",
		"changeset": "changeset",
		"version-packages": "changeset version",
		"docs": "cd docs && npm run dev",
		"docs:check-links": "cd docs && npm run check",
		"docs:rename-file": "cd docs && npm run rename",
		"report-issue": "node scripts/report-issue.js",
		"storybook": "cd webview-ui && npm run storybook"
	},
	"lint-staged": {
		"src/shared/storage/state-keys.ts": [
			"node scripts/generate-state-proto.mjs",
			"git add proto/coder/state.proto"
		],
		"*": [
			"biome check --write --staged --no-errors-on-unmatched --files-ignore-unknown=true"
		]
	},
